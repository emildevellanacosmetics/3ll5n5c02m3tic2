<meta name="robots" content="noindex,nofollow">
<script type="application/json" id="StoreJSON">
{
  "stores": [
  {%- for article in blogs.store-data.articles -%}
    {%- assign article_content = article.content | newline_to_br | replace: "<br />", "<br>" | remove: "<span>" |remove: "</span>" -%}
    {%- assign content_split = article_content | remove: "<p>" | remove: "</p>" -%}
    {%- assign content_split = content_split | split: "<br>" -%}
    {
     "Fcilty_nam": "{{ article.title | strip }}",
     "Street_add": "{{ content_split[0] | strip }}",
     "Locality": "{{ content_split[1] | strip }}",
     "Ycoord": "{{ content_split[2] | strip }}",
     "Xcoord": "{{ content_split[3] | strip }}",
     "uuid": "{{ forloop.index }}"
    }{% unless forloop.last %},{% endunless %}
  {%- endfor -%}
  ]
}
</script>